        "person = gender_guesser.Detector()\n",
        "genders = []\n",
        "first_names = payroll_df['First Name'].astype(str)\n",
        "for name in first_names:\n",
        "    person_gender = person.get_gender(name.capitalize())\n",
        "    genders.append(person_gender)\n",
        "\n",
        "gender = pd.Series(genders)"
